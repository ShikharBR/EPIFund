@model Inview.Epi.EpiFund.Domain.ViewModel.UserNoteModel
<fieldset id="pnlUserInformation">
    <legend>User Note</legend>

    @Html.HiddenFor(m => m.UserId)
    @Html.HiddenFor(s=>s.UserNoteId)
    <div class="control-group">
        @Html.LabelFor(s => s.Date, new { @class = "control-label", style = "font-weight:bold;" })*
        <div class="controls">
            @Html.DisplayFor(s => s.Date)@Html.ValidationMessageFor(s => s.Date, "")
        </div>
    </div>
    <div class="control-group">
        @Html.LabelFor(s => s.Note, new { @class = "control-label", style = "font-weight:bold;" })*
        <div class="controls">
            @Html.TextAreaFor(s => s.Note, 5, 100, new { style="resize:none;" })@Html.ValidationMessageFor(s => s.Note, "")
        </div>
    </div>
</fieldset>
