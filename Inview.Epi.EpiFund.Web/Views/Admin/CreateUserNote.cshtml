@model Inview.Epi.EpiFund.Domain.ViewModel.UserNoteModel
@{
    ViewBag.Title = "Create User Note";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

@using (Html.BeginForm("CreateUserNote", "Admin", FormMethod.Post))
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(false)
    <br />
    @Html.EditorForModel()

    <input type="submit" class="btn btn-primary" id="btnSave" name="btnSave" value="Save Note" />
 
}