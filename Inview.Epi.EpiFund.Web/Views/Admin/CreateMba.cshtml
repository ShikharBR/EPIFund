@model Inview.Epi.EpiFund.Domain.ViewModel.MbaViewModel
@{
    ViewBag.Title = "Create MBA";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Create MBA</h2>
@using (Html.BeginForm("CreateMba", "Admin", FormMethod.Post))
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(false)
    <br />
    @Html.EditorForModel()

    <input type="submit" class="btn btn-primary" id="btnSave" name="btnSave" value="Save MBA" /> 
}